{
  "name": "shed-preact",
  "version": "0.0.1",
  "description": "Steve's Preact starter kit",
  "main": "server.js",
  "scripts": {
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "babel": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest",
    "babel-plugin-transform-react-jsx": "latest"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-router": "^2.5.4"
  },
  "engines": {
    "node": "6.9.x"
  },
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "repository": {
    "url": "https://glitch.com/#!/project/shed-preact"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "preact"
  ]
}